{
    "name": "Qiita Notifications",
    "version": "0.0.1",
    "description": "Keep up with Qiita anywhere on the web.",
    "browser_action": {
        "default_icon": "images/16.png",
        "popup": "build/popup.html"
    },
    "icons": {
        "16": "images/16.png",
        "48": "images/48.png"
    },
    "permissions": [
        "tabs",
        "http://qiita.com/*"
    ]
}
